{
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": ["src/mar/**/*.js", "src/mar/**/*.jsx"],
    "coverageDirectory": "target/reports/coverage",
    "coveragePathIgnorePatterns": ["src/site/*.*"],
    "coverageReporters": ["html", "lcov", "cobertura", "text-summary"],
    "modulePaths": ["<rootDir>/src/", "<rootDir>/node_modules/mar-med-list-js/lib"]
  }